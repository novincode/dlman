{
  "name": "@dlman/extension",
  "displayName": "DLMan - Download Manager",
  "version": "1.9.1",
  "private": true,
  "type": "module",
  "description": "Browser extension for DLMan - Modern download manager integration",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "prebuild": "node scripts/copy-icons.js",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:chrome": "wxt build -b chrome",
    "build:all": "npm run build && npm run build:firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:all": "npm run zip && npm run zip:firefox",
    "release": "npm run build:all && npm run zip:all",
    "compile": "tsc --noEmit",
    "copy-icons": "node scripts/copy-icons.js",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "^1.1.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sonner": "^1.7.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.277",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.3",
    "wxt": "^0.19.0"
  }
}
